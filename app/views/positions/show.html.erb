<div class="col-md-6 col-md-offset-3">
  <div class="page-header">
    <h1>Cargo - Detalhes</h1>
  </div>
  <div class="panel panel-body">
    <p id="notice"><%= notice %></p>
    <p>
      <h3>Nome: <%= @position.name %></h3>
    </p>
  </div>
  <div class="page-footer pull-right">
    <%= link_to 'Editar Cargo', edit_position_path(@position), class: "btn btn-primary" %>
    <%= link_to 'Voltar', positions_path, class: "btn btn-default" %>
  </div>
</div>


