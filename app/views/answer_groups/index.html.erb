<p id="notice"><%= notice %></p>

<h1>Answer Groups</h1>

<table>
  <thead>
    <tr>
      <th>Evaluation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answer_groups.each do |answer_group| %>
      <tr>
        <td><%= answer_group.evaluation %></td>
        <td><%= link_to 'Show', answer_group %></td>
        <td><%= link_to 'Edit', edit_answer_group_path(answer_group) %></td>
        <td><%= link_to 'Destroy', answer_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer Group', new_answer_group_path %>
