<p id="notice"><%= notice %></p>

<h1>Evaluation Cycles</h1>

<table>
  <thead>
    <tr>
      <th>Initial date</th>
      <th>End date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluation_cycles.each do |evaluation_cycle| %>
      <tr>
        <td><%= evaluation_cycle.initial_date %></td>
        <td><%= evaluation_cycle.end_date %></td>
        <td><%= link_to 'Show', evaluation_cycle %></td>
        <td><%= link_to 'Edit', edit_evaluation_cycle_path(evaluation_cycle) %></td>
        <td><%= link_to 'Destroy', evaluation_cycle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evaluation Cycle', new_evaluation_cycle_path %>
