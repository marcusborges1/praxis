<div class="col-md-10 col-md-offset-1">
  <div class="panel panel-primary">
    <div class="panel-heading">Ciclo de Avaliação - Ranking</div>
    <p id="notice"><%= notice %></p>
    <div class="panel-body">
      <table class="table table-striped">
        <thead>
          <tr>
            <th>Posição</th>
            <th>Nome</th>
            <th>Pontuação</th>
          </tr>
        </thead>
        <tbody>
          <% @ranking.each_with_index do |data, index| %>
          <tr>
            <td><%= index + 1 %></td>
            <td><%= data["user_name"] %></td>
            <td><%= data["score"] %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <div class="panel-footer">
      <%= link_to 'Voltar', evaluation_cycles_path, class: "btn btn-default" %>
    </div>
  </div>
</div>
